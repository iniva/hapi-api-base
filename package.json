{
  "name": "hapi-api-base",
  "description": "Opinionated base code for a Hapi API",
  "version": "2.0.0",
  "main": "init.js",
  "repository": "git@github.com:iniva/hapi-api-base.git",
  "author": "Tony <tonyblast@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.10.0 <9",
    "yarn": ">=1.6.0"
  },
  "scripts": {
    "audit": "npm i --package-lock-only && npm audit",
    "clean": "rm -rf dist",
    "commit": "git-cz",
    "build": "yarn clean && babel src --out-dir dist --source-maps --copy-files",
    "build:docs": "DEBUG=docs* node docs/builder.js",
    "bump:version": "yarn version && yarn build:docs",
    "lint": "eslint src hooks",
    "hooks:message": "node hooks/index.js --action=message",
    "start": "nodemon init.js",
    "start:dist": "node dist/index.js",
    "start:debug": "NODE_ENV=development DEBUG=* yarn start",
    "start:development": "NODE_ENV=development yarn start",
    "start:production": "NODE_ENV=production yarn start:dist",
    "test": "echo \"no tests for now\""
  },
  "dependencies": {
    "@babel/runtime": "7.x.x",
    "axios": "0.x.x",
    "blipp": "3.x.x",
    "boom": "7.x.x",
    "debug": "3.x.x",
    "dotenv": "5.x.x",
    "good": "8.x.x",
    "good-console": "7.x.x",
    "good-squeeze": "5.x.x",
    "hapi": "17.x.x",
    "hapi-nosql-mongoose": "1.x.x",
    "ioredis": "3.x.x",
    "joi": "13.x.x",
    "lodash": "4.x.x",
    "mongoose": "5.x.x",
    "sequelize": "4.x.x",
    "sqlite3": "4.x.x",
    "webpack-merge": "4.x.x"
  },
  "devDependencies": {
    "@babel/cli": "7.x.x",
    "@babel/core": "7.x.x",
    "@babel/plugin-proposal-object-rest-spread": "7.x.x",
    "@babel/plugin-transform-runtime": "7.x.x",
    "@babel/polyfill": "7.x.x",
    "@babel/preset-env": "7.x.x",
    "@babel/register": "7.x.x",
    "babel-plugin-module-resolver": "3.x.x",
    "commitizen": "2.x.x",
    "cz-conventional-changelog": "2.x.x",
    "eslint": "4.x.x",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-plugin-import": "2.x.x",
    "husky": "^1.0.0-rc.13",
    "nodemon": "1.x.x",
    "refs-compiler": "1.x.x"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
