{
  "name": "hapi-api-base",
  "description": "Opinionated base code for a Hapi API",
  "version": "2.9.1",
  "main": "init.js",
  "repository": "git@github.com:iniva/hapi-api-base.git",
  "author": "Tony <tonyblast@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=10 <11 || >=12",
    "yarn": ">=1.16.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "commit": "git-cz",
    "build": "yarn clean && babel src --out-dir dist --source-maps --copy-files",
    "build:docs": "DEBUG=docs* node docs/builder.js",
    "bump:version": "yarn version && yarn build:docs",
    "lint": "eslint src hooks",
    "hooks:message": "node hooks/index.js --action=message",
    "start": "nodemon init.js",
    "start:dist": "node dist/index.js",
    "start:debug": "NODE_ENV=development DEBUG=* yarn start",
    "start:development": "NODE_ENV=development yarn start",
    "start:production": "NODE_ENV=production yarn start:dist",
    "test": "BABEL_ENV=test NODE_ENV=test jest",
    "test:coverage": "yarn test --coverage",
    "test:coveralls": "yarn test --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@hapi/boom": "^8.0.1",
    "@hapi/good": "^8.2.3",
    "@hapi/good-console": "^8.1.2",
    "@hapi/good-squeeze": "^5.2.1",
    "@hapi/hapi": "^19.1.1",
    "@hapi/joi": "^16.1.4",
    "agentkeepalive": "^4.0.2",
    "blipp": "^4.0.1",
    "core-js": "2",
    "debug": "^4.1.0",
    "dotenv": "^8.1.0",
    "got": "^9.6.0",
    "hapi-nosql-mongoose": "^1.0.7",
    "hapi-pino": "^6.1.0",
    "ioredis": "^4.14.1",
    "mongoose": "^5.7.5",
    "slack-webhooks-handler": "^1.2.2",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@types/jest": "^24.0.18",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-airbnb": "^4.0.1",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.6",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "refs-compiler": "^2.0.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
